{
  "expo": {
    "name": "Tamil Community Portal",
    "slug": "tamilcommunity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "tamilcommunity",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "13e75112-4d3d-4c30-8dee-d04e13216105"
      }
    },
    "android": {
      "package": "com.deepakthirdeye.tamilcommunity"
    }
  }
}

